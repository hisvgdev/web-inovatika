{
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "semi": false,
  "singleQuote": true,
  "tabWidth": 4,
  "useTabs": false,
  "printWidth": 100,
  "importOrder": [
      "<BUILTIN_MODULES>",
      "<THIRD_PARTY_MODULES>",
      "",
      "^@/layouts/(.*)$",
      "^@/templates/(.*)$",
      "^@/modals/(.*)$",
      "^@/forms/(.*)$",
      "^@/organisms/(.*)$",
      "^@/molecules/(.*)$",
      "^@/atoms/(.*)$",
      "",
      "^@/lib/(.*)$",
      "^@/validation/(.*)$",
      "",
      "^@/types/(.*)$",
      "<TYPES>",
      "",
      "^[./]"
  ]
}
